#include <stdio.h>

int main(void){

	int *lPtr;

	int value1 = 9, value2 = 5;

	lPtr = &value1;

	printf("%d\n", *lPtr);
	printf("%d\n", &lPtr);

	*lPtr = value2;

	printf("%d\n", *lPtr);

	printf("%d\n", &value1);
	printf("%d\n", &value2);

	printf("%d", &lPtr);

	return 0;
}


