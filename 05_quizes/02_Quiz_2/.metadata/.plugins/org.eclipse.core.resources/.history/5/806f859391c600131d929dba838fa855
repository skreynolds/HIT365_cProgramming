#include <stdio.h>

int main(void){

	char fantasy[] = "The cat sat on the mat.";

	/* Answer to part a */
	printf("%c\n\n", fantasy[0]);

	/* Answer to part b */
	printf("%d\n", sizeof(fantasy));
	printf("%d\n", sizeof(fantasy[0]));
	printf("%c\n", fantasy[sizeof(fantasy)/sizeof(fantasy[0])]);

	return 0;
}


